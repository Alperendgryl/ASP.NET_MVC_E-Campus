
@{
    ViewBag.Title = "Projects";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/StyleSheet.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
</head>
<body class="bg-white">
    <div class="row" style="padding-right:20px; padding-left:20px;">
        <div class="col bg-white">
            <div class="card">
                <div class="card-header bg-blue text-white">
                    <h4 class="text-light" style="height: 8px; font-family: Arial; font-size:18px">Leaded Projects</h4>
                </div>
                <li class="list-group-item border px-3 pb-3 pt-1">
                    <br>
                    <table class="col text-dark" border="1" cellspacing="2" cellpadding="20">
                        <tr>
                            <th>Lead Ssn</th>
                            <th>Project Name</th>
                            <th>Budget</th>
                            <th>Start Date</th>
                            <th>End Date</th>
                            <th>Subject</th>
                            <th>Constr D.Name</th>
                        </tr>
                        @foreach (var Project in Model)
                        {
                            <tr>
                                <td>@Project.leadSsn</td>
                                <td>@Project.prName</td>
                                <td>@Project.budget</td>
                                <td>@Project.startDate</td>
                                <td>@Project.endDate</td>
                                <td>@Project.subject</td>
                                <td>@Project.contrDName</td>
                            </tr>
                        }
                    </table>


                </li>
            </div>
        </div>
    </div>
</body>
</html>

